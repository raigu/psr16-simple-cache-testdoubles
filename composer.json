{
    "name": "raigu/psr16-simple-cache-testdoubles",
    "description": "Test Doubles for PSR-16 simple cache",
    "type": "library",
    "provide": {
        "psr/simple-cache-implementation": "1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rait Kapp"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raigu\\TestDouble\\Psr16\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raigu\\TestDouble\\Psr16\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4||^8.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "specification": "./vendor/bin/phpunit --testdox",
        "coverage": "./vendor/bin/phpunit --coverage-text"
    }
}
